<nav class="navbar navbar-light navbar-dark bg-success">
  <a class="navbar-brand"  href="#">ユーザー一覧</a>
</nav>

 
<div class="card-columns m-3">
  <% @users.each do |user| %>
    <div class="card" style="width: 100%;">
    <%= link_to user_path(user) do %>
      <%= attachment_image_tag user, :profile_image, fallback: "no-image.png" %>
    <% end %>
      <div class="card-body">
        <h3 class="card-title"><%=link_to user.username, user_path(user) %></h3>
        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
      </div>
    </div>
  <% end %>
</div>  